{
    "env": {
        "node": true,
        "es6": true
    },
    "extends": [
      "airbnb-base",
      "eslint:recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "comma-dangle": "off",
      "react/jsx-filename-extension": "off"
    }
}
